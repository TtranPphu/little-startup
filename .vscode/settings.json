{
  "java.configuration.updateBuildConfiguration": "automatic",
  "java.compile.nullAnalysis.mode": "automatic",
  "java.format.settings.profile": "GoogleStyle",
  "editor.formatOnSave": true,
  "editor.insertSpaces": true,
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.rulers": [88]
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.rulers": [120]
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[dockercompose]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "python.defaultInterpreterPath": "./.venv/bin/python",
  "files.exclude": {
    "db-mongodb": true,
    "example/node_modules": true,
    "example/dist": true,
    "example/package-lock.json": true,
    "example/.parcel-cache": true,
    "example/.env": true,
    "example/uv.lock": true
  },
  "github.codespaces.devcontainerChangedNotificationStyle": "none",
  "docker.extension.dockerEngineAvailabilityPrompt": false,
  "editor.renderWhitespace": "all",
  "code-eol.newlineCharacter": "¬",
  "code-eol.returnCharacter": "¤",
  "code-eol.crlfCharacter": "¤¬",
  // https://github.com/ryanoasis/nerd-fonts/releases/download/v3.3.0/Hermit.zip
  "editor.fontFamily": "Hurmit Nerd Font",
  "editor.fontSize": 12,
  "terminal.integrated.fontFamily": "Hurmit Nerd Font",
  "terminal.integrated.fontSize": 12
}
