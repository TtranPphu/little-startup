{
  "name": "little-example-devcontainer",
  "initializeCommand": "sh initialize.sh",
  "postCreateCommand": "sh .devcontainer/little-example/post-create.sh",
  "dockerComposeFile": "../../docker-compose.yml",
  "service": "little-example-devcontainer",
  "runServices": ["little-example-devcontainer"],
  "workspaceFolder": "/workspaces/little-startup",
  "portsAttributes": {
    "1234": {
      "label": "little-example",
      "onAutoForward": "notify"
    }
  },
  "customizations": {
    "vscode": {
      "settings": {
        // https://github.com/ryanoasis/nerd-fonts/releases/download/v3.3.0/CascadiaCode.zip
        "editor.fontFamily": "Consolas, 'Courier New', monospace, 'CaskaydiaCove Nerd Font'",
        "terminal.integrated.fontFamily": "CaskaydiaCove Nerd Font",
        "workbench.colorTheme": "Default Dark Modern"
      },
      "extensions": [
        "chekweitan.compare-view",
        "cweijan.vscode-mysql-client2",
        "eriklynd.json-tools",
        "esbenp.prettier-vscode",
        "github.codespaces",
        "github.copilot",
        "github.vscode-pull-request-github",
        "mechatroner.rainbow-csv",
        "medo64.render-crlf",
        "ms-azuretools.vscode-docker",
        "ms-python.black-formatter",
        "ms-python.python",
        "ms-python.vscode-pylance",
        "ms-toolsai.jupyter",
        "rust-lang.rust-analyzer",
        "tamasfe.even-better-toml",
        "usernamehw.errorlens",
        "vadimcn.vscode-lldb"
      ]
    }
  }
}
